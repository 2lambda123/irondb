<div class="container-fluid p-0" id="results">
  <table class="table table-striped table-bordered table-hover ">
    <thead class="thead-dark">
      <tr>
        <th>Name</th>
        <th>Group</th>
        <th>Measurement Technique</th>
        <th>Major Elements</th>
        <th>Minor Elements</th>
        <th>Trace Elements</th>
        <th>Title</th>
        <th>Authors</th>
        <th>Pg. #</th> 
        <th>Journal</th> 
        <th>Issue</th>
        <th>Year Published</th> 
      </tr>
    </thead>
    <tbody>
      <% for(var i=0; i < Entries.length; i++) { %>
      <tr>
        <p hidden="true"><%= Entries[i].entry_id %></p>
        <th><%= Entries[i].meteorite_name %></th>
        <th><%= Entries[i].classification_group %></th>
        <th><%= Entries[i].technique %></th>
        <th><%= Entries[i].major_elements %></th>
        <th><%= Entries[i].minor_elements %></th>
        <th><%= Entries[i].trace_elements %></th>
        <th><%= Entries[i].title %></th>
        <th><%= Entries[i].authors %></th>
        <th><%= Entries[i].page_number %></th> 
        <th><%= Entries[i].journal_name %></th> 
        <th><%= Entries[i].issue_number %></th>
        <th><%= Entries[i].published_year %></th> 
      </tr>
      <% } %>
    </tbody>
  </table>
</div>